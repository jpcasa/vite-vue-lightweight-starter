{
  "name": "vite-vue3-typescript-eslint-prettier",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev-ts dev-vite",
    "dev-ts": "vue-tsc --watch --noEmit",
    "dev-vite": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint \"src/**/*.vue\" \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.vue\" \"src/**/*.ts\"",
    "preview": "vite preview",
    "test": "vitest --dom"
  },
  "dependencies": {
    "@prettier/plugin-pug": "^3.0.0",
    "eslint-plugin-vue-pug": "^0.6.2",
    "happy-dom": "6.0.4",
    "pinia": "^2.1.6",
    "postcss": "^8.4.38",
    "pug": "^3.0.3",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.3.2",
    "@storybook/vue3-vite": "^7.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/language-plugin-pug": "^1.8.27",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-pug": "^1.2.5",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "storybook": "^7.3.2",
    "svgo": "^3.0.2",
    "typescript": "^5.2.0",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pug": "^0.3.2",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.8"
  }
}
